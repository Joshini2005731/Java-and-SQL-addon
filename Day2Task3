import java.util.Random;
import java.util.Scanner;
class Main {
     public static void main(String[] args) {
         Scanner scan=new Scanner(System.in);
          System.out.println("enter your name");
         String a=scan.next();
         System.out.println("enter your mobilenumber");
         int b=scan.nextInt();
         System.out.println("enter your e-mail");
         String c=scan.next();
         System.out.println("enter your address");
         String d=scan.next();
         System.out.println("enter your skillset");
         String e=scan.next();
         System.out.println("enter your project description");
         String f=scan.next();
         System.out.println("my name is:"+a);
         System.out.println("my mobile number is:"+b);
         System.out.println("my name is email id:"+c);
         System.out.println("my address is:"+d);
         System.out.println("my skillset is:"+e);
         System.out.println("my project description is:"+f);
}
}
