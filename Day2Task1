package addon;

public class demomethod {
	
	public static void Details(String Name,String Job,String Package) {
		
	
		System.out.println("My name is "+Name+ "My job is "+Job+ "My package is "+Package);
				
	}

	public static void main(String[] args) {
		Details("josh","it","20000");
		Details("mirdhu","it","30000");
		
	}
}
